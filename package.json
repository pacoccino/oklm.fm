{
  "name": "oklm-default",
  "version": "1.0.0",
  "description": "oklm web radio",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/server.js",
    "install": "grunt buildProd",
    "build": "grunt build",
    "start-worker":"./startWorker.sh",
    "start-webs":"./startWebs.sh",
    "stop-webs":"forever stop server/web.js",
    "deploy-nginx-config": "npm run _nginx-clean-config && npm run _nginx-copy-config && npm run _nginx-symbolic-link && npm run _nginx-restart",
    "_nginx-copy-config": "sudo cp -R config/nginx/conf.d/ /etc/nginx && sudo cp -R config/nginx/sites-available/ /etc/nginx",
    "_nginx-symbolic-link": "sudo ln -s /etc/nginx/sites-available/oklm.fm.conf /etc/nginx/sites-enabled/oklm.fm.conf",
    "_nginx-clean-config": "sudo rm -rf /etc/nginx/confi.d/oklm.fm-upstream.conf /etc/nginx/sites-available/oklm.fm.conf /etc/nginx/sites-enabled/oklm.fm.conf",
    "_nginx-restart": "sudo service nginx restart"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://5169959@git.dc0.gpaas.net/default.git"
  },
  "engines": {
    "node": "~4.2.2"
  },
  "author": "pakokrew",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.14.2",
    "cookie-parser": "^1.4.0",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "forever": "^0.15.1",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-less": "^1.1.0",
    "grunt-contrib-uglify": "^0.11.0",
    "grunt-contrib-watch": "^0.6.1",
    "load-grunt-tasks": "^3.4.0",
    "request": "^2.67.0",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.4.3"
  },
  "devDependencies": {}
}
